{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6",
         "es2019",
         "esnext"
      ],
      "target": "es2019",
      "module": "commonjs",
      "strict": false,
      "moduleResolution": "node",
      "outDir": "./dist",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "baseUrl": "./"
   }
}